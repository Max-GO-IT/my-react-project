{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "parser": "babel",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json"
        }
      }
    ]
  }